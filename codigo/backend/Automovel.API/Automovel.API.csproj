    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>f87fa136-3c56-484a-aadc-5b59e5356cc4</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
